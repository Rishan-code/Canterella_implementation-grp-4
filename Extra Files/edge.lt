Edge {
  write("Data Atoms") {
    $atom:A  $mol:Edge  @atom:C  0.0  0.0  0.0
    $atom:B  $mol:Edge  @atom:C  1.5  0.0  0.0
  }

  write("Data Bonds") {
    $bond:1  @bond:C-C  $atom:A  $atom:B
  }

  write_once("Data Masses") {
    @atom:C  12.011

  write_once("Data Bond Coeffs") {
    @bond:C-C  300.0  1.5
  }

  write_once("Data Pair Coeffs") {
    @atom:C  @atom:C  0.1  3.5
  }
}
